{
    "name": "template-typescript",
    "version": "2.0.0",
    "description": "Template for TypeScript projects",
    "author": "Vinicius Egidio <me@vinicius.io>",
    "license": "MIT",
    "type": "module",
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "engines": {
        "node": ">=20.10.0"
    },
    "dependencies": {
        "@types/config": "3.3.5",
        "@types/jest": "30.0.0",
        "@types/node": "25.1.0",
        "@typescript-eslint/eslint-plugin": "8.54.0",
        "@typescript-eslint/parser": "8.54.0",
        "config": "4.2.0",
        "eslint": "9.39.2",
        "eslint-plugin-prettier": "5.5.5",
        "jest": "30.2.0",
        "pkgroll": "2.23.0",
        "prettier": "3.8.1",
        "ts-jest": "29.4.6",
        "tsx": "4.21.0"
    },
    "devDependencies": {
        "typescript": "5.9.3",
        "winston": "3.19.0"
    },
    "scripts": {
        "build": "pkgroll --minify --target=node20.10.0",
        "lint": "eslint . --ext .ts",
        "start": "tsx src/index.ts",
        "test": "jest",
        "upgrade": "pnpm dlx npm-check-updates -u"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vegidio/template-typescript"
    }
}
